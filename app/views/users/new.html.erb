<h1>New User</h1>

<%= form_for @user do |f| %>
  <p>
    <b>First name</b>
    <%= f.text_field :first_name %>
  </p>

  <p>
    <b>Last name</b>
    <%= f.text_field :last_name %>
  </p>

  <p>
    <b>Email</b>
    <%= f.text_field :email %>
  </p>

  <p>
    <b>Password</b>
    <%= f.password_field :password %>
  </p>

  <%= f.submit %> 
<% end %>